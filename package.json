{
  "name": "serverless-api",
  "version": "1.0.0",
  "description": "`serverless deploy --aws-profile serverless`",
  "main": "handler.js",
  "scripts": {
    "deploy": "serverless deploy --aws-profile serverless --stage dev --region us-east-1",
    "testzips": "serverless invoke -f getZip -l --path ./test/test-zips.json",
    "testreps": "serverless invoke -f getRepById -l --path ./test/test-reps.json",
    "testsens": "serverless invoke -f getSenById -l --path ./test/test-sens.json"
  },
